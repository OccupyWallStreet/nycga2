	position: relative;
}

div#ics-link {
	visibility: hidden; 
	position: absolute;
	padding: 10px;
	background-color: #FFF;	
	border-radius: 3px 3px 3px 3px;
	box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);
	z-index: 100;
	width: 250px;
}

div#ics-link input {
	width: 244px;
	padding: 2px;
	background: none repeat scroll 0 0 #FFFFFF;
	border: 1px inset #CCCCCC;
	border-radius: 3px 3px 3px 3px;
	color: #555555;
}

div#ics-link input:focus {
	box-shadow: 0 0 7px rgba(31, 179, 221, 0.7);
}

a#close-ics {
	position: absolute;
	top: 10px;
	right: 10px;
}