#: admin/em-options.php:561 admin/em-options.php:564 admin/em-options.php:615
#: admin/em-options.php:719 admin/em-options.php:722 admin/em-options.php:769
#: admin/em-options.php:772 admin/em-options.php:861 admin/em-options.php:864
#: admin/em-options.php:955 admin/em-options.php:958 admin/em-options.php:1121
#: widgets/em-events.php:139 widgets/em-locations.php:109
msgid "Descending"
msgstr "Z->A"

# @ dbem
#: admin/em-options.php:574 admin/em-options.php:634 admin/em-options.php:1136
msgid ""
"When Events Manager displays lists of events the default behaviour is "
"ordering by start date in ascending order. To change this, modify the values "
"above."
msgstr ""
"Keď správca udalostí zobrazuje zoznam udalostí, je predvolené usporiadanie "
"podľa ich začaiatku vo vzostupnom poradí. Ak to chcete zmeniť, upravte tieto "
"možnosti."

# @ dbem
#: admin/em-options.php:580 admin/em-options.php:736 admin/em-options.php:807
msgid "General settings"
msgstr "Hlavné nastavenia"

# @ dbem
