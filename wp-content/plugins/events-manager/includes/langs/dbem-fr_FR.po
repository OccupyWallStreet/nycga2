msgid "Ascending"
msgstr "Ascendant"

# @ dbem
#: admin/em-options.php:561 admin/em-options.php:564 admin/em-options.php:615
#: admin/em-options.php:715 admin/em-options.php:718 admin/em-options.php:765
#: admin/em-options.php:768 admin/em-options.php:853 admin/em-options.php:856
#: admin/em-options.php:941 admin/em-options.php:944 admin/em-options.php:1088
#: widgets/em-events.php:139 widgets/em-locations.php:109
msgid "Descending"
msgstr "Descendant"

# @ dbem
#: admin/em-options.php:574 admin/em-options.php:634 admin/em-options.php:1103
msgid ""
"When Events Manager displays lists of events the default behaviour is "
"ordering by start date in ascending order. To change this, modify the values "
"above."
msgstr ""
